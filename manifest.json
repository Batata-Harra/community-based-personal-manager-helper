{
  "manifest_version": 2,
  "name": "My New Tab Extension",
  "version": "0.1",
  "description": "For now it gonna be a unique clock new tab with a clock per hour and timeline",
  "author": "Batata Harra",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "https://api.hackernoon.com/",
    "https://raw.githubusercontent.com/kevinabouhanna",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "128": "icon128.png"
    },
    "default_title": "Google Mail",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  }
}
